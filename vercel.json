{
    "version": 2,
    "builds": [
      {
        "src": "projectmain.html",
        "use": "@vercel/static"
      },
      {
        "src": "mainstyle.css",
        "use": "@vercel/static"
      },
      {
        "src": "scripts/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/projectmain.html"
      }
    ]
  }